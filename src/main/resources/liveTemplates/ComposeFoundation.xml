<templateSet group="Compose Foundation Live Templates">

    <template name="col"
              value="androidx.compose.foundation.layout.Column(&#10;verticalArrangement = androidx.compose.foundation.layout.Arrangement.Top,&#10;modifier = androidx.compose.ui.Modifier&#10;) {$CURSOR$&#10;}"
